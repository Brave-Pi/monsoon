# common

-lib tink_http
-lib tink_io
-lib mime
#-cp ../../mime-db
#--macro mime.Mime.init()
-cp ../src
-main Run
--each

# neko

-lib tink_tcp
-lib tink_runloop
-lib tink_concurrent
-D concurrent
-D embed
-neko bin/neko/index.n

# java

--next
-lib hxjava
-lib tink_tcp
-lib tink_runloop
-lib tink_concurrent
-D concurrent
-D embed
-java bin/java/

# cpp

--next
-lib tink_tcp
-lib tink_runloop
-lib tink_concurrent
-D concurrent
-D embed
-cpp bin/cpp/
 
# mod_neko

--next
-neko bin/mod_neko/index.n

# nodejs

--next
-lib hxnodejs
-js bin/node/index.js
-dce full

# php

--next
-dce full
-php bin/php/
-cmd sed -i 's/${"DEFAULT"}/$hDEFAULT/g' bin/php/lib/tink/io/_Worker/Worker_Impl_.class.php
